{
    "Collection": {
		"prefix": "mat-collection",
		"body": [
			"<div class=\"collection\">",
            "\t<a href=\"${1}\" class=\"collection-item\">${2}</a>",
            "</div>"
		],
		"description": "Materialize collection"
    },
    "Collection with badge": {
		"prefix": "mat-collection-badge",
		"body": [
			"<div class=\"collection\">",
            "\t<a href=\"${1}\" class=\"collection-item\"><span class=\"badge\">${2}</span>${3}</a>",
            "</div>"
		],
		"description": "Materialize collection with badge"
	},
    "Responsive image": {
		"prefix": "mat-responsive-img",
		"body": [
            "<img class=\"responsive-img\" src=\"${1}\" id=\"\" alt=\"${2}\">"
		],
		"description": "Materialize responsive image"
    },
    "Circular image": {
		"prefix": "mat-circle-responsive-img",
		"body": [
            "<img class=\"circle responsive-img\" src=\"${1}\" id=\"\" alt=\"${2}\">"
		],
		"description": "Materialize circular image"
	}

}
